#docker-compose up -d 启动完后，需要进入容器进行一下配置

#CMD in mysql-master
create user 'backup'@'%' identified by '123456';
GRANT REPLICATION SLAVE ON *.* to 'backup'@'%' ;
show master status; 


#CMD in mysql-slave
CHANGE MASTER TO MASTER_HOST='192.168.199.113',MASTER_PORT=3307,MASTER_USER='backup',MASTER_PASSWORD='123456',MASTER_LOG_FILE='mysql-bin.000003',MASTER_LOG_POS=859;
start slave;
show slave status\G;

